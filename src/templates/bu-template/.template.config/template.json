{
  "$schema": "http://json.schemastore.org/template",
  "author": "Oleksii Nikiforov",
  "classifications": [
    "build-project",
    "Template"
  ],
  "identity": "NikiforovAll.BuildProject.Template",
  "name": "Build Project [na-bu]",
  "description": "Creates a build project to manage multiple projects via docker-compose.",
  "shortName": "na-bu",
  "sourceName": "NikiforovAll.BU.Template",
  "preferNameDirectory": true,
  "defaultName": "build-project",
  "tags": {
    "language": "bash",
    "type": "project"
  },
  "symbols":{
    "includeSolutionFile": {
        "type": "parameter",
        "datatype": "bool",
        "defaultValue": "false"
    }
  },
  "guids": [
  ],
  "sources":[{
    "modifiers": [{
      "condition": "(!includeSolutionFile)",
      "exclude": [ "NikiforovAll.BuildProject.Template.sln"]
      }
    ]
  }],
  "postActions": []
}
