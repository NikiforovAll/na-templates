{
  "IdentityProvider": {
    "ExternalUrl": "",
    "ClientId": "",
    "Audience": ""
  },
  "UseStubAuthentication": true,
  "ConnectionStrings": {
    "DefaultConnection": "User ID=postgres;Password=postgres;Server=localhost;Database=ca-template-db;Pooling=true;"
  },
  "AllowedOrigins": [
    "*"
  ],
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "restrictedToMinimumLevel": "Verbose"
              }
            }
          ]
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  }
}
